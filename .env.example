# Yahoo Fantasy API Credentials
# Get these from: https://developer.yahoo.com/apps/
YAHOO_CLIENT_ID=your_client_id_here
YAHOO_CLIENT_SECRET=your_client_secret_here
YAHOO_REDIRECT_URI=http://localhost:5000/yahoo/auth/callback

# Flask Configuration
FLASK_SECRET_KEY=your_secret_key_here
FLASK_ENV=development
FLASK_DEBUG=True

# Yahoo Fantasy Database (optional - defaults to SQLite)
YAHOO_DATABASE_URL=sqlite:///yahoo_fantasy.db

# Optional: Main Database URL
# DATABASE_URL=sqlite:///fantasy.db